<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="199.0" prefWidth="212.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bazyprojekt.okna.modale.M_dodajPostController">
   <children>
      <VBox layoutX="10.0" layoutY="10.0" spacing="4.0">
         <children>
            <Label fx:id="lTytul" alignment="TOP_LEFT" text="Dodaj posterunek">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label fx:id="lNumer" text="#000000">
               <graphic>
                  <Label text="Numer">
                     <font>
                        <Font name="System Italic" size="12.0" />
                     </font>
                     <padding>
                        <Insets right="8.0" />
                     </padding>
                  </Label>
               </graphic>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Label>
            <HBox spacing="8.0">
               <children>
                  <VBox />
                  <VBox>
                     <children>
                        <Label text="Sektor" />
                        <ComboBox fx:id="cbSektory" onAction="#wczytajID" prefWidth="150.0">
                           <VBox.margin>
                              <Insets bottom="8.0" />
                           </VBox.margin>
                        </ComboBox>
                     </children>
                  </VBox>
               </children>
            </HBox>
            <Separator>
               <VBox.margin>
                  <Insets bottom="8.0" top="8.0" />
               </VBox.margin>
            </Separator>
            <HBox alignment="CENTER_RIGHT" spacing="16.0">
               <children>
                  <Button fx:id="bOK" disable="true" minWidth="64.0" mnemonicParsing="false" onAction="#handleOK" text="OK" />
                  <Button cancelButton="true" minWidth="64.0" mnemonicParsing="false" onAction="#handleOK" text="Anuluj" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
